{
  "dart.lineLength": 120,
  "editor.defaultFormatter": "Dart-Code.dart-code",
  "editor.formatOnSave": false,
  "editor.formatOnType": false,
  "editor.rulers": [
    80,
    120
  ],
  "editor.tabSize": 2,
  "editor.wordWrap": "off",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "java.configuration.updateBuildConfiguration": "interactive",
  "search.exclude": {
    "**/android/.gradle/": true,
    "**/android/app/build/": true,
    "**/android/build/": true,
    "**/android/gradle/": true,
    "**/build/": true,
    "**/ios/Flutter/": true,
    "**/ios/Flutter/AppFrameworkInfo.plist": true,
    "**/ios/Flutter/Flutter.podspec": true,
    "**/ios/Flutter/flutter_export_environment.sh": true,
    "**/ios/Pods/": true,
    "**/ios/Runner/GeneratedPluginRegistrant.{h,m}": true
  }
}
